var slots_count = 3
var new_spawner = null

func _ready():
	var last_position = null
	var spawner = load(\"res://Scenes/LocationObjectCards/LocationObjectSpawner.tscn\").instance()
	add_child(spawner)
	spawner.position.x = 683 - ((spawner.get_node(\"Sprite\").texture.get_width()/2) / slots_count)
	var next_position_x = spawner.position.x + (spawner.get_node(\"Sprite\").texture.get_width()/2)
	spawner.position.y = 768/2
	var i = 1
	while i < slots_count:
		new_spawner = load(\"res://Scenes/LocationObjectCards/LocationObjectSpawner.tscn\").instance()
		add_child(new_spawner)
		new_spawner.position.x = next_position_x
		new_spawner.position.y = 768/2
		next_position_x = new_spawner.position.x + (new_spawner.get_node(\"Sprite\").texture.get_width()/2)
		i += 1
	var axe = load(\"res://Scenes/Cards/Tool.tscn\").instance()
	add_child(axe)
	axe.position.x = 1366/2
"

